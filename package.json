{
  "name": "node-redis-client",
  "description": "nodejs版本的redis客户端",
  "author": "chenchunjiang",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server   --inline --hot --env.dev --config webpack.config.js",
    "dev": "webpack-dev-server --inline --hot --env.dev",
    "build": "rimraf dist && webpack -p --progress --hide-modules ",
    "package": "electron-packager . redis-vue --platform=darwin --arch=x64 --icon=app.icns --out=./dist --asar --app-version=1.0.0 --ignore=\"(dist|src|docs|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\"",
    "winpack": "electron-packager . bugshell --platform=win32 --arch=x64 --out bugshell-app --overwrite --ignore=\"(node_modules|node-redis-client-darwin-x64|bugshell-app)\""
  },
  "dependencies": {
    "codemirror": "^5.58.3",
    "element-ui": "^2.14.1",
    "ftp": "^0.3.10",
    "ioredis": "^4.17.3",
    "log4js": "^1.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "ssh2": "^0.8.9",
    "ssh2-sftp-client": "^5.3.2",
    "utf8": "^1.0.0",
    "vue": "^2.5.16",
    "vxe-table": "^1.0.0",
    "xe-utils": "^3.0.2",
    "xterm": "^4.9.0",
    "xterm-addon-fit": "^0.4.0"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.0",
    "babel-preset-vue-app": "^1.2.0",
    "css-loader": "^0.27.0",
    "dgram": "^1.0.1",
    "electron": "^10.0.0",
    "electron-packager": "^13.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.24.1",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.5.4",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "style-loader": "^0.13.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.3.0",
    "vue-router": "^3.4.7",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "main": "main.js"
}
